{{ ansible_managed | comment }}

[Unit]
Description=VideoOnLAN Service
After=network.target

[Service]
Type=forking
User={{ media_vlc_user }}
ExecStart=/usr/bin/vlc --daemon --syslog -I http --http-port {{ media_vlc_port }} --http-password {{ media_vlc_password }}
Restart=always

[Install]
WantedBy=multi-user.target
